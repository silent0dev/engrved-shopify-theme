  window.Shoppad.apps.infiniteoptions.beforeReady = function(subscribe) {
    subscribe('appLoad', function(event) {
     console.log('appLoad', event);

      setTimeout(function () {
		console.log('inputs stuff -',  jQuery("input[type=radio].spb-productoptiontextcolor"));
        jQuery("input[type=radio].spb-productoptiontextcolor").each(function (
          key,
          item
        ) {
          jQuery(item).click(function (e) {
            console.log('click event')
            setTimeout(function () {
              console.log('timeout')
              var itemParentValue = jQuery(item).parent().parent();
              if (
                jQuery(itemParentValue[0].childNodes[1]).attr("value") !==
                jQuery(item).val()
              ) {
                  console.log('Double click')
                jQuery(item).click();
              }
            }, 100);
          });
        });
      }, 500);
    });
  };
